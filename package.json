  "homepage": "https://github.com/popkirby/chiery#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "source-map-support": "^0.4.0"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "chalk": "^1.1.3",
